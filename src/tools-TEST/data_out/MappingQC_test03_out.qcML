                        <tr>
                          <td><xsl:value-of select="@accession"/></td>
                          <td><span title="{@description}"><xsl:value-of select="@name"/></span></td>
                          <td><xsl:value-of select="@value"/></td>
                        </tr>
                      </xsl:for-each>
                    </xsl:for-each>
                  </table>
